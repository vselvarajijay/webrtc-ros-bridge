# ROS2 Bridge Docker Environment Variables
# Copy this file to .env and fill in your values

# ROS2 Configuration
ROS_DOMAIN_ID=0
RMW_IMPLEMENTATION=rmw_fastrtps_cpp

# Frodobots SDK Configuration
SDK_API_TOKEN=your_api_token_here
BOT_SLUG=your-bot-slug-here
MISSION_SLUG=your-mission-slug-here

# SDK Settings (optional)
MAP_ZOOM_LEVEL=18
IMAGE_FORMAT=png
IMAGE_QUALITY=1.0
DEBUG=false

# Network Configuration (internal Docker network)
# SDK_URL is automatically set to http://frodobots_sdk:8000
# WEBRTC_SIGNALING_PORT is automatically set to 8080
